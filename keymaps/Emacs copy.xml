<keymap version="1" name="Emacs copy" parent="Emacs">
  <action id="$Cut">
    <keyboard-shortcut first-keystroke="shift delete" />
  </action>
  <action id="ActivateBuildToolWindow" />
  <action id="ActivateDatabaseToolWindow">
    <keyboard-shortcut first-keystroke="alt 8" />
  </action>
  <action id="ActivateServicesToolWindow" />
  <action id="ActivateUnitTestsToolWindow">
    <keyboard-shortcut first-keystroke="alt 0" />
  </action>
  <action id="BuildSolutionAction">
    <keyboard-shortcut first-keystroke="shift ctrl b" />
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="ctrl b" />
  </action>
  <action id="EditorCloneCaretAbove">
    <keyboard-shortcut first-keystroke="shift ctrl comma" />
  </action>
  <action id="EditorCloneCaretBelow">
    <keyboard-shortcut first-keystroke="shift ctrl period" />
  </action>
  <action id="EditorCut">
    <keyboard-shortcut first-keystroke="shift delete" />
  </action>
  <action id="ExpandLiveTemplateCustom">
    <keyboard-shortcut first-keystroke="tab" />
    <keyboard-shortcut first-keystroke="ctrl j" />
  </action>
  <action id="ExportToTextFile" />
  <action id="FileChooser.NewFolder">
    <keyboard-shortcut first-keystroke="alt insert" />
  </action>
  <action id="GotoAction">
    <keyboard-shortcut first-keystroke="shift ctrl a" />
    <keyboard-shortcut first-keystroke="alt x" />
  </action>
  <action id="GotoSuperMethod" />
  <action id="GotoTypeDeclaration">
    <mouse-shortcut keystroke="shift control button1" />
  </action>
  <action id="InsertLiveTemplate" />
  <action id="IntroduceParameter" />
  <action id="NextSplitter">
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="o" />
    <keyboard-shortcut first-keystroke="alt o" />
  </action>
  <action id="ReSharperGotoImplementation">
    <keyboard-shortcut first-keystroke="shift ctrl alt g" />
  </action>
  <action id="Resume">
    <keyboard-shortcut first-keystroke="f9" />
    <keyboard-shortcut first-keystroke="f8" />
  </action>
  <action id="RiderUnitTestDebugContextAction">
    <keyboard-shortcut first-keystroke="ctrl semicolon" second-keystroke="d" />
    <keyboard-shortcut first-keystroke="ctrl semicolon" second-keystroke="ctrl d" />
    <keyboard-shortcut first-keystroke="ctrl u" second-keystroke="shift ctrl f10" />
  </action>
  <action id="RiderUnitTestRunContextAction">
    <keyboard-shortcut first-keystroke="ctrl semicolon" second-keystroke="r" />
    <keyboard-shortcut first-keystroke="ctrl semicolon" second-keystroke="ctrl r" />
    <keyboard-shortcut first-keystroke="shift ctrl f10" />
  </action>
  <action id="StepOver">
    <keyboard-shortcut first-keystroke="f10" />
  </action>
  <action id="XDebugger.AttachToLocalProcess">
    <keyboard-shortcut first-keystroke="ctrl alt p" />
  </action>
</keymap>